{
  "name": "quill",
  "displayName": "Quill RAG Extension",
  "description": "VSCode extension for RAG functionality",
  "version": "0.0.9",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ebrahimdev/ra-prod.git"
  },
  "license": "MIT",
  "contributes": {
    "commands": [
      {
        "command": "quill.query",
        "title": "Quill: Query RAG"
      },
      {
        "command": "quill.login",
        "title": "Quill: Login"
      },
      {
        "command": "quill.register",
        "title": "Quill: Register"
      },
      {
        "command": "quill.logout",
        "title": "Quill: Logout"
      },
      {
        "command": "quill.status",
        "title": "Quill: Auth Status"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "build": "node build.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}